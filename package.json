{
  "name": "learn-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export -o build && node index.js",
    "build-handler": "ts-node index.ts",
    "deploy:cdn": "cdk --app 'ts-node -r dotenv/config -T aws/bin/deploy-cdn.ts dotenv_config_path=.env' deploy 'QestWebCdnEnvironment' --progress events --require-approval never",
    "deploy:dev": "cdk --app 'ts-node -r dotenv/config -T aws/bin/deploy.ts dotenv_config_path=.env' deploy 'QestWebAppStackDev' --progress events --require-approval never",
    "deploy:prod": "cdk --app 'ts-node -r dotenv/config -T aws/bin/deploy.ts dotenv_config_path=.env' deploy 'QestWebAppStackProd' --progress events --require-approval never",
    "start": "next start"
  },
  "dependencies": {
    "@apollo/client": "^3.3.11",
    "@apollo/react-hooks": "^4.0.0",
    "apollo-boost": "^0.4.9",
    "graphql": "^15.5.0",
    "next": "^10.0.0",
    "next-with-apollo": "^5.1.1",
    "node-fetch": "^2.6.1",
    "path": "^0.12.7",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@aws-cdk/aws-apigateway": "1.89.0",
    "@aws-cdk/aws-cloudfront": "1.89.0",
    "@aws-cdk/aws-cloudfront-origins": "1.89.0",
    "@aws-cdk/aws-lambda": "1.89.0",
    "@aws-cdk/aws-route53-targets": "1.89.0",
    "@aws-cdk/aws-s3": "1.89.0",
    "@aws-cdk/aws-s3-deployment": "1.89.0",
    "@aws-cdk/core": "1.89.0",
    "@babel/core": "^7.12.17",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.13",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.17",
    "@sls-next/lambda-at-edge": "1.7.0",
    "@sls-next/serverless-component": "1.18.0",
    "@types/node": "^14.14.30",
    "@types/react": "^17.0.2",
    "@types/styled-components": "^5.1.7",
    "aws-cdk": "1.89.0",
    "babel-plugin-styled-components": "^1.12.0",
    "dotenv": "^8.2.0",
    "node": "^15.8.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  }
}
