{
  "name": "qest-web",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "dev": "yarn generate:locale & next dev",
    "generate:types": "graphql-codegen --config codegen.yml",
    "generate:locale": "i18next-scanner --config generate-locale.config.js",
    "build": "next build",
    "deploy:dev": "cdk --app 'ts-node -r dotenv/config -T aws/bin/deploy.ts dotenv_config_path=.env' deploy 'QestWebAppStackDev' --progress events --require-approval never",
    "deploy:prod": "cdk --app 'ts-node -r dotenv/config -T aws/bin/deploy.ts dotenv_config_path=.env' deploy 'QestWebAppStackProd' --progress events --require-approval never",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "ts:check": "tsc",
    "test": "npm-run-all -l -p ts:check lint prettier",
    "code:fix": "npm-run-all -l -p lint:fix prettier:fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@styled-icons/boxicons-logos": "^10.23.0",
    "graphql": "^15.5.1",
    "graphql-request": "^3.4.0",
    "rss-parser": "^3.12.0",
    "next": "^11.0.0",
    "next-i18next": "^8.5.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-responsive-masonry": "^2.1.2",
    "react-slick": "^0.28.1",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@aws-cdk/aws-apigateway": "1.89.0",
    "@aws-cdk/aws-cloudfront": "1.89.0",
    "@aws-cdk/aws-cloudfront-origins": "1.89.0",
    "@aws-cdk/aws-lambda": "1.89.0",
    "@aws-cdk/aws-route53-targets": "1.89.0",
    "@aws-cdk/aws-s3": "1.89.0",
    "@aws-cdk/aws-s3-deployment": "1.89.0",
    "@aws-cdk/core": "1.89.0",
    "@graphql-codegen/cli": "^1.21.3",
    "@graphql-codegen/fragment-matcher": "^2.0.1",
    "@graphql-codegen/introspection": "^1.18.1",
    "@graphql-codegen/typescript": "^1.21.1",
    "@graphql-codegen/typescript-operations": "^1.17.15",
    "@types/node": "^14.14.30",
    "@types/react": "^17.0.2",
    "@types/react-anchor-link-smooth-scroll": "^1.0.1",
    "@types/react-dom": "17.0.2",
    "@types/react-scroll": "^1.8.2",
    "@types/react-slick": "^0.23.4",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "aws-cdk": "1.89.0",
    "babel-plugin-styled-components": "^1.12.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.30.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.0",
    "i18next-scanner": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.3.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.5"
  }
}
