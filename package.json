{
	"name": "learn-starter",
	"version": "0.1.0",
	"private": true,
	"engines": {
		"node": ">=14 <=15.8.0"
	},
	"scripts": {
		"dev": "npm run posts:fetch && next dev",
		"generate": "graphql-codegen --config codegen.yml",
		"build": "npm run posts:fetch && next build && next export -o build",
		"deploy:dev": "cdk --app 'ts-node -r dotenv/config -T aws/bin/deploy.ts dotenv_config_path=.env' deploy 'QestWebAppStackDev' --progress events --require-approval never",
		"deploy:prod": "cdk --app 'ts-node -r dotenv/config -T aws/bin/deploy.ts dotenv_config_path=.env' deploy 'QestWebAppStackProd' --progress events --require-approval never",
		"start": "next start",
		"lint": "eslint",
		"lint:fix": "eslint --fix",
		"prettier": "prettier --check .",
		"prettier:fix": "prettier --write .",
		"code:check": "npm-run-all -l -p lint prettier",
		"generateLocale": "ts-node -r dotenv/config -P tsconfig.generate.json -T scripts/generateLocale.ts dotenv_config_path=.env",
		"posts:fetch": "ts-node -r dotenv/config -P tsconfig.generate.json -T  scripts/fetchMediumPosts dotenv_config_path=.env",
		"code:fix": "npm-run-all -l -p lint:fix prettier:fix"
	},
	"dependencies": {
		"@apollo/client": "^3.3.11",
		"@apollo/react-hooks": "^4.0.0",
		"@styled-icons/boxicons-logos": "^10.23.0",
		"@types/react-anchor-link-smooth-scroll": "^1.0.1",
		"apollo-boost": "^0.4.9",
		"graphql": "^15.5.0",
		"i18next": "^19.9.2",
		"next": "^10.0.0",
		"next-with-apollo": "^5.1.1",
		"node-fetch": "^2.6.1",
		"path": "^0.12.7",
		"react": "17.0.1",
		"react-anchor-link-smooth-scroll": "^1.0.12",
		"react-dom": "17.0.1",
		"react-i18next": "^11.8.10",
		"react-responsive-masonry": "^2.1.2",
		"react-slick": "^0.28.1",
		"styled-components": "^5.2.1"
	},
	"devDependencies": {
		"@aws-cdk/aws-apigateway": "1.89.0",
		"@aws-cdk/aws-cloudfront": "1.89.0",
		"@aws-cdk/aws-cloudfront-origins": "1.89.0",
		"@aws-cdk/aws-lambda": "1.89.0",
		"@aws-cdk/aws-route53-targets": "1.89.0",
		"@aws-cdk/aws-s3": "1.89.0",
		"@aws-cdk/aws-s3-deployment": "1.89.0",
		"@aws-cdk/core": "1.89.0",
		"@babel/core": "^7.12.17",
		"@babel/plugin-proposal-class-properties": "^7.12.13",
		"@babel/plugin-proposal-export-default-from": "^7.12.13",
		"@babel/plugin-proposal-export-namespace-from": "^7.12.13",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/polyfill": "^7.12.1",
		"@babel/preset-env": "^7.12.17",
		"@babel/preset-react": "^7.12.13",
		"@babel/preset-typescript": "^7.12.17",
		"@graphql-codegen/cli": "^1.21.3",
		"@graphql-codegen/fragment-matcher": "^2.0.1",
		"@graphql-codegen/introspection": "^1.18.1",
		"@graphql-codegen/near-operation-file-preset": "^1.17.13",
		"@graphql-codegen/typescript": "^1.21.1",
		"@graphql-codegen/typescript-operations": "^1.17.15",
		"@graphql-codegen/typescript-react-apollo": "^2.2.3",
		"@types/node": "^14.14.30",
		"@types/node-fetch": "^2.5.8",
		"@types/react": "^17.0.2",
		"@types/react-dom": "17.0.2",
		"@types/react-slick": "^0.23.4",
		"@types/styled-components": "^5.1.7",
		"@typescript-eslint/eslint-plugin": "^4.0.0",
		"@typescript-eslint/parser": "^4.0.0",
		"aws-cdk": "1.89.0",
		"babel-eslint": "^10.0.0",
		"babel-plugin-styled-components": "^1.12.0",
		"dotenv": "^8.2.0",
		"eslint": "^7.5.0",
		"eslint-config-prettier": "^7.1.0",
		"eslint-config-react-app": "^6.0.0",
		"eslint-plugin-flowtype": "^5.2.0",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-jest": "^24.0.0",
		"eslint-plugin-jsx-a11y": "^6.3.1",
		"eslint-plugin-react": "^7.20.3",
		"eslint-plugin-react-hooks": "^4.0.8",
		"eslint-plugin-testing-library": "^3.10.1",
		"npm-run-all": "^4.1.5",
		"prettier": "2.2.1",
		"rss-parser": "^3.12.0",
		"ts-node": "^9.1.1",
		"typescript": "^4.1.5"
	}
}
